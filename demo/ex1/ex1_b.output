    set NUM LEVELS                   = 10
    set TOLERANCE FOR MESH ADAPTION  = 1
    set TOP FRAC                     = 0.1
  end
end
subsection Geometry
  set ATOMIC COORDINATES FILE = coordinates.inp
  set DOMAIN VECTORS FILE     = domainVectors.inp
  set NATOMS                  = 2
  set NATOM TYPES             = 1
  subsection Optimization
