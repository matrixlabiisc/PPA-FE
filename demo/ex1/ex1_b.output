    set CELL OPT                  = false
    set CELL STRESS               = false
    set FORCE TOL                 = 1e-4
    set ION FORCE                 = false
    set ION OPT                   = true
    set ION RELAX FLAGS FILE      = relaxationFlags.inp
    set NON SELF CONSISTENT FORCE = false
    set REUSE WFC                 = false
